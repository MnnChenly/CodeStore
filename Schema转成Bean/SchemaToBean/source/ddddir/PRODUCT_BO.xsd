			<xs:element name="IS_NULLABLE" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>是否非空</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ATTR_VALUE_TYPE" type="xs:string" minOccurs="0">
				<xs:annotation>
