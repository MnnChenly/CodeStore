				</xs:annotation>
			</xs:element>
			<xs:element name="ATTR_FORMAT" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="ATTR_LENGTH" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>长度</xs:documentation>
				</xs:annotation>
			</xs:element>
