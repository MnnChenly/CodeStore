<xs:schema xmlns="http://www.bean.cmp.ffcs.cn/product_bo/" xmlns:comm="http://www.bean.cmp.ffcs.cn/comm/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.bean.cmp.ffcs.cn/product_bo/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://www.bean.cmp.ffcs.cn/comm/" schemaLocation="../base/COMM.xsd"/>
	<xs:complexType name="PRODUCT_TYPE">
		<xs:annotation>
			<xs:documentation>产品规格类型</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="PRODUCT_ID" type="xs:string">
				<xs:annotation>
					<xs:documentation>产品规格ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PRODUCT_NAME" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>产品名称</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PROD_FUNC_TYPE" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>类型</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PRODUCT_DESC" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>产品描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EXT_PROD_ID" type="xs:string" minOccurs="0">
