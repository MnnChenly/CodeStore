					<xs:documentation>属性值名称</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ATTR_VALUE_DESC" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>属性值说明</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ATTRVALUE" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>属性值</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
